{
  "name": "elm-webpack",
  "version": "1.0.0",
  "description": "elm boilerplate with webpack",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf dist/",
    "api": "node api.js",
    "dev": "webpack-dev-server --hot --env.dev --content-base dist/ --progress",
    "start": "npm-run-all --parallel api dev",
    "prebuild": "npm run clean",
    "build": "webpack --env.prod"
  },
  "author": "Ruben Costa",
  "license": "MIT",
  "devDependencies": {
    "css-loader": "^0.23.1",
    "elm-hot-loader": "^0.3.3",
    "elm-webpack-loader": "^3.0.3",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "json-server": "^0.8.17",
    "npm-run-all": "^2.3.0",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.1.0-beta.20",
    "webpack-dev-server": "^2.1.0-beta.0"
  },
  "dependencies": {
    "ace-css": "^1.1.0",
    "font-awesome": "^4.6.3"
  }
}
